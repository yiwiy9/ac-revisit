{
  "name": "ac-revisit",
  "build": { "dockerfile": "Dockerfile" },
  "workspaceFolder": "/workspaces/ac-revisit",
  "remoteUser": "node",
  "updateRemoteUserUID": true,

  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },

  "postCreateCommand": "node -v && npm -v && codex --version"
}
